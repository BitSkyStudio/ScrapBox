{
BitmapFont: {
	default-font: {
		file: default.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
	small-font: {
		file: font-small.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
}
Color: {
	black: {
		r: 0
		g: 0
		b: 0
		a: 1
	}
	white: {
		r: 1
		g: 1
		b: 1
		a: 1
	}
	green: {
		r: 0
		g: 1
		b: 0
		a: 1
	}
	red: {
		r: 1
		g: 0
		b: 0
		a: 1
	}
	blue: {
		r: 0
		g: 0
		b: 1
		a: 1
	}
	grey: {
		r: 0.32
		g: 0.32
		b: 0.32
		a: 1
	}
	vis-blue: {
		r: 0.105
		g: 0.631
		b: 0.886
		a: 1
	}
	vis-red: {
		r: 0.862
		g: 0
		b: 0.047
		a: 1
	}
	menuitem: {
		r: 0.65
		g: 0.65
		b: 0.65
		a: 1
	}
	link-label: {
		r: 0.105
		g: 0.631
		b: 0.886
		a: 1
	}
}
TintedDrawable: {
	dialogDim: {
		name: white
		color: {
			r: 0
			g: 0
			b: 0
			a: 0.45
		}
	}
}
ButtonStyle: {
	default: {
		up: button
		down: button-sel
	}
	blue: {
		up: button-blue
		down: button-blue-down
	}
	toggle: {
		up: button
		down: button-down
		checked: button-down
	}
}
CheckBoxStyle: {
	default: {
		checkboxOn: check-on
		checkboxOff: check-off
		font: default-font
		fontColor: white
		disabledFontColor: grey
	}
	radio: {
		checkboxOn: radio-on
		checkboxOff: radio-off
		font: default-font
		fontColor: white
		disabledFontColor: grey
	}
}
ImageButtonStyle: {
	default: {
		up: button
		down: button-down
	}
	blue: {
		up: button-blue
		down: button-blue-down
	}
	toggle: {
		up: button
		down: button-down
		checked: button-down
	}
}
ImageTextButtonStyle: {
	default: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		up: button
		down: button-down
		over: button-over
		disabled: button
	}
	blue: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		up: button-blue
		down: button-blue-down
		over: button-blue-over
		disabled: button
	}
	toggle: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		up: button
		down: button-down
		over: button-over
		disabled: button
		checked: button-down
	}
}
LabelStyle: {
	default: {
		font: default-font
		fontColor: white
	}
	link-label: {
		font: default-font
		fontColor: link-label
	}
	small: {
		font: small-font
		fontColor: white
	}
	menuitem-shortcut: {
		font: default-font
		fontColor: menuitem
	}
}
ListStyle: {
	default: {
		font: default-font
		fontColorSelected: white
		fontColorUnselected: white
		selection: button
		down: button
		over: button-sel
		background: window
	}
}
ProgressBarStyle: {
	default-horizontal: {
		background: progressbar
		knob: progressbar-filled
		knobBefore: progressbar-filled
	}
	default-vertical: {
		background: progressbar-vertical
		knob: progressbar-filled-vertical
		knobBefore: progressbar-filled-vertical
	}
}
ScrollPaneStyle: {
	default: {
		background: window
		hScroll: button
		hScrollKnob: button-sel
		vScroll: button
		vScrollKnob: button-sel
	}
	list: {
		hScroll: scroll-horizontal
		hScrollKnob: scroll-knob-horizontal
		vScroll: scroll
		vScrollKnob: scroll-knob-vertical
	}
}
SelectBoxStyle: {
	default: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		background: default-select
		scrollStyle: default
		listStyle: default
	}
}
SliderStyle: {
	default-horizontal: {
		knobOver: slider-knob-over
		knobDown: slider-knob-down
		background: slider
		knob: slider-knob
		disabledKnob: slider-knob-disabled
	}
	default-vertical: {
		knobOver: slider-knob-over
		knobDown: slider-knob-down
		background: slider-vertical
		knob: slider-knob
		disabledKnob: slider-knob-disabled
	}
}
SplitPaneStyle: {
	default-horizontal: {
		handle: splitpane
	}
	default-vertical: {
		handle: splitpane-vertical
	}
}
TextButtonStyle: {
	default: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		up: button
		down: button
		over: button-sel
		disabled: button
	}
	blue: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		up: button-blue
		down: button-blue-down
		over: button-blue-over
		disabled: button
	}
	toggle: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		up: button
		down: button-down
		over: button-over
		disabled: button
		checked: button-down
	}
}
TextFieldStyle: {
	default: {
		font: default-font
		fontColor: white
		disabledFontColor: grey
		background: textfield
		cursor: cursor
		selection: selection
		messageFont: default-font
		messageFontColor: grey
	}
	small: {
		font: small-font
		fontColor: white
		disabledFontColor: grey
		background: textfield
		cursor: cursor
		selection: selection
		messageFont: default-font
		messageFontColor: grey
	}
}
TextTooltipStyle: {
	default: {
		label: default
		background: default-pane
	}
}
TouchpadStyle: {
	default: {
		background: default-pane
		knob: touchpad-knob
	}
}
TreeStyle: {
	default: {
		plus: tree-plus
		minus: tree-minus
		over: tree-over
		selection: tree-selection
	}
}
WindowStyle: {
	default: {
		background: window
		titleFont: default-font
		titleFontColor: white
	}
	resizable: {
		background: window-resizable
		titleFont: default-font
		titleFontColor: white
	}
	noborder: {
		background: window-noborder
		titleFont: default-font
		titleFontColor: white
	}
	dialog: {
		background: window
		titleFont: default-font
		titleFontColor: white
		stageBackground: dialogDim
	}
}
}